{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "documentDB",
      "name": "inputDocument",
      "databaseName": "%dictionary_db_NAME%",
      "collectionName": "%akelius_definitions_NAME%",
      "connection": "akelius-dictionary_DOCUMENTDB",
      "direction": "in",
      "sqlQuery": "SELECT * FROM dict WHERE dict.id = {id}"
    },
    {
      "type": "documentDB",
      "name": "outputDocument",
      "databaseName": "%dictionary_db_NAME%",
      "collectionName": "%akelius_definitions_NAME%",
      "createIfNotExists": false,
      "connection": "akelius-dictionary_DOCUMENTDB",
      "direction": "out"
    }
  ]
}